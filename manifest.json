{
  "manifest_version": 3,
  "name": "skip test",
  "version": "1.0",
  "description": "test",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": ["background.js", "popup.js"]
  }]
}